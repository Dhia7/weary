{
  "name": "wear-backend",
  "version": "1.0.0",
  "description": "Backend API for Wear e-commerce platform",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "jest",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:restart": "docker-compose restart",
    "db:logs": "docker-compose logs -f",
    "db:reset": "docker-compose down -v && docker-compose up -d",
    "db:simple": "docker-compose -f docker-compose.simple.yml up -d",
    "db:simple:down": "docker-compose -f docker-compose.simple.yml down"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "e-commerce",
    "api",
    "express",
    "postgresql",
    "pgbouncer"
  ],
  "author": "",
  "license": "MIT"
}
